@model List<Core.Models.Project>


<div class="card mb-3" >
  <div class="row g-0">
    <div class="col-md-3">
            <img src="/images/cv.png" class="img-fluid rounded-start custom-img-size" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">

               @foreach (var project in Model)
                {
                    <h5 class="card-title">@project.Titel</h5>
                    <p class="card-text">@project.Beskrivning</p>
                    <p class="card-text"><small class="text-muted">Skapades den @project.SkapadDen</small></p>
                }
                



      </div>
    </div>
  </div>
</div>


